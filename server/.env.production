# Server Configuration
NODE_ENV=production
PORT=3001

# Piston API Configuration (Docker internal network)
PISTON_URL=http://piston:2000

# Database Configuration
DATABASE_PATH=/app/data/codeforge.db

# CORS Configuration
CORS_ORIGIN=https://yourdomain.com

# Execution Limits
MAX_EXECUTION_TIME=3000
MAX_CODE_LENGTH=50000

# Logging
LOG_LEVEL=warn